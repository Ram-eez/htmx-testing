package components 

import "HTMX/models"


templ CountryNames(countries []models.Country) {
    <div>
        for _, country := range countries{
            <div>
            <h1>The Country is { country.Name }</h1>
            </div>
        }
    </div>

}

templ Button_eg(text string) {
    <div>
    <button class="button">{ text }</button>
    </div>
}